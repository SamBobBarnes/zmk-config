        };

        onshape_layer_3 {
            //              -----------------------------------------------------------------------------------    -----------------------------------------------------------------------------------
            //  -----------|           |           |           |           |           |           |           ||||           |           |           |     /     |     *     |     -     |           |-----------
            // |    LU     |    tab    |   Constr  |  Project  |  Extrude  |  Revolve  |   Trim    |___________||||___________|           |     7     |     8     |     9     |     +     |           |    LU     |
            // |____LD_____|    esc    |    Arc    |   Line    | Dimension |  Fillet   |   Rect    |           ||||           |           |     4     |     5     |     6     |    ENT    |           |____LD_____|
            //             |   shift   |           |           |  Circle   |           |           |-----------||||-----------|           |     1     |     2     |     3     |    ENT    |           |
